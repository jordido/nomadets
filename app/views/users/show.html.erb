<h1>User</h1>

<table class="table table_stripted">
			<th>First name</th>
			<th>Last name</th>
			<th>Email</th>
			<th>Address</th>
			<th>City</th>
			<th>Region</th>
			<th>Country</th>
			<th>Type</th>
	<% @user do |u| %>
		<tr>
			<td><%= u.name %></td>
			<td><%= u.last_name %></td>
			<td><%= u.email %></td>
			<td><%= u.address %></td>
			<td><%= u.city %></td>
			<td><%= u.region %></td>
			<td><%= u.country %></td>
			<td><%= u.type %></td>
		</tr>
	<% end %>
</table>
<br/>
<%= link_to "Back", users_path %>